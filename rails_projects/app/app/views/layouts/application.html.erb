<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>   
   
  </head>

  <body>
    <% if user_signed_in? %>
      <%= link_to 'Logout',destroy_user_session_path,method: :delete %>
    <% else %>
      <%= link_to 'Sign in',new_user_session_path %>
      <%= link_to 'Sign up',new_user_registration_path %>
    <% end %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
